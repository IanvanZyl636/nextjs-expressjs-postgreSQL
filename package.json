{
  "name": "@nextjs-expressjs-postgre-sql/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "generate:db:env": "dotenv -e .env ts-node scripts/generate-env.ts",
    "docker:start": "docker-compose -f docker/app/docker-compose.yml up -d --no-deps --build",
    "docker:jenkins:start": "npm run generate:db:env && docker-compose --profile jenkins up -d --no-deps --build",
    "docker:postgresql:start": "npm run generate:db:env && docker-compose --profile postgresql up -d --no-deps --build",
    "prisma:migrate": "npm run generate:db:connection && npx prisma migrate dev --schema=apps/backend/src/integrations/prisma",
    "prisma:reset": "dotenv -e apps/backend/.env -- npx prisma migrate reset --schema=apps/backend/src/integrations/prisma",
    "prisma:reset:force": "dotenv -e apps/backend/.env -- npx prisma migrate reset --force --schema=apps/backend/src/integrations/prisma",
    "prisma:deploy": "dotenv -e apps/backend/.env -- npx prisma migrate deploy --schema=apps/backend/src/integrations/prisma",
    "prisma:seed": "dotenv -e apps/backend/.env -- npx prisma db seed --schema=apps/backend/src/integrations/prisma"
  },
  "private": true,
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@eslint/eslintrc": "^2.1.1",
    "@eslint/js": "^9.8.0",
