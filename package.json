{
  "name": "@nextjs-expressjs-postgre-sql/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "npx nx run-many --target=build --no-cloud",
    "test": "npx nx run-many --target=test --coverage --passWithNoTests --no-cloud --skip-nx-cache",
    "e2e": "npx nx run-many --target=e2e --no-cloud",
    "lint": "npx nx run-many --target=lint --no-cloud",
    "start:frontend": "cd ./apps/frontend && next start",
    "start:backend": "node apps/backend/dist/main.js",
    "local:test": "npm run prisma:reset:force:test && npm run test",
    "local:prod:start": "dotenv -e .env -- node apps/backend/dist/main.js",
    "local:frontend:start": "nx run frontend:dev",
    "local:backend:start": "npm run docker:start && npm run prisma:deploy && nx run backend:serve --skip-nx-cache",
    "docker:start": "docker compose up -d --no-deps --build",
    "docker:jenkins:start": "docker compose --profile jenkins up -d --no-deps --build",
    "docker:cloudflare:start": "docker compose --profile cloudflare up -d --no-deps --build",
    "prisma:migrate": "npx prisma migrate dev --schema=apps/backend/src/integrations/prisma",
    "prisma:reset": "npx prisma migrate reset --schema=apps/backend/src/integrations/prisma",
    "prisma:reset:force": "npx prisma migrate reset --force --schema=apps/backend/src/integrations/prisma",
    "prisma:reset:test": "dotenv -e .env.test -- npx prisma migrate reset --schema=apps/backend/src/integrations/prisma",
    "prisma:reset:force:test": "dotenv -e .env.test -- npx prisma migrate reset --force --schema=apps/backend/src/integrations/prisma",
    "prisma:deploy": "npx prisma migrate deploy --schema=apps/backend/src/integrations/prisma",
    "prisma:seed": "npx prisma db seed --schema=apps/backend/src/integrations/prisma"
  },
  "private": true,
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@eslint/eslintrc": "^2.1.1",
    "@eslint/js": "^9.8.0",
    "@next/eslint-plugin-next": "^15.2.4",
    "@nx/devkit": "21.0.3",
    "@nx/eslint": "21.0.3",
    "@nx/eslint-plugin": "21.0.3",
    "@nx/express": "^21.0.3",
    "@nx/jest": "21.0.3",
    "@nx/js": "^21.0.3",
    "@nx/next": "^21.0.3",
    "@nx/node": "21.0.3",
    "@nx/playwright": "21.0.3",
    "@nx/web": "21.0.3",
    "@nx/webpack": "21.0.3",
    "@playwright/test": "^1.36.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@svgr/webpack": "^8.0.1",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@swc/jest": "~0.2.36",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "18.16.9",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "autoprefixer": "10.4.13",
    "babel-jest": "^29.7.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.8.0",
    "eslint-config-next": "^15.2.4",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "5.0.0",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "jsonc-eslint-parser": "^2.1.0",
    "nx": "21.0.3",
    "postcss": "8.4.38",
    "prettier": "^2.6.2",
    "prisma": "^6.8.2",
    "react-refresh": "^0.10.0",
    "tailwindcss": "3.4.3",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.19.0",
    "webpack-cli": "^5.1.4"
  },
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "dependencies": {
    "@prisma/client": "^6.8.2",
    "zod": "^3.25.20"
  }
